schema: "../api/schema.gql"
documents:
  - "./**/*.{graphql,js,ts,jsx,tsx}"
  - "!node_modules"
generates:
  ./features/Data/generated/graphql.ts:
    plugins:
      - typescript
      - typescript-operations
      - typescript-react-query
    config:
      fetcher:
        func: "../useFetchData#useFetchData"
        isReactHook: true
        skipTypeName: true
