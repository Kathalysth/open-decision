{
  "jsc": {
    "target": "es2017",
    "parser": {
      "syntax": "typescript",
      "dynamicImport": true
    },
    "transform": {},
    "keepClassNames": true,
    "externalHelpers": true,
    "loose": true
  },
  "module": {
    "type": "commonjs",
    "strict": true,
    "noInterop": true
  },
  "sourceMaps": true,
  "exclude": [
    ".*.config.ts?$",
    ".*.spec.tsx?$",
    ".*.test.tsx?$",
    "./src/jest-setup.ts$",
    "./**/jest-setup.ts$",
    ".*.js$",
    "node_modules"
  ]
}
