{
  "name": "@open-decision/interpreter",
  "version": "0.0.1",
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "/lib",
    "/src"
  ],
  "description": "The foundational types classes shared by all parts of the open decision application.",
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf lib && vite build && pnpm tsc -p tsconfig.build.json",
    "dev": "pnpm tsc -p tsconfig.build.json & vite build --watch",
    "prepublish": "pnpm build",
    "prettier": "prettier --check src/",
    "prettier:fix": "prettier --write src/",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "verify": "run-p prettier lint",
    "verify:fix": "yarn prettier:fix && yarn lint:fix",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@xstate/react": "^3.0.0",
    "remeda": "^0.0.32",
    "valtio": "^1.3.1",
    "xstate": "^4.31.0"
  },
  "peerDependencies": {
    "@open-decision/type-classes": "workspace:^",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@open-decision/type-classes": "workspace:^",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vitest": "^0.9.2"
  }
}
