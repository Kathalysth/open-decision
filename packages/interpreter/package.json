{
  "name": "@open-decision/interpreter",
  "version": "0.0.1",
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "/lib",
    "/src"
  ],
  "description": "The foundational types classes shared by all parts of the open decision application.",
  "sideEffects": false,
  "scripts": {
    "build": "pnpm build --filter @open-decision/interpreter^... && rm -rf lib & tsc --composite false --incremental false & vite build",
    "dev": "tsc --composite false --incremental false & vite build --watch",
    "prepublish": "pnpm build",
    "prettier": "prettier --check src/",
    "prettier:fix": "prettier --write src/",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "verify": "run-p prettier lint",
    "verify:fix": "yarn prettier:fix && yarn lint:fix"
  },
  "dependencies": {
    "valtio": "^1.3.1"
  },
  "peerDependencies": {
    "@open-decision/type-classes": "workspace:^",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@open-decision/type-classes": "workspace:^",
    "typescript": "^4.6.2",
    "vite": "^2.8.6"
  }
}
