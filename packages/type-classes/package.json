{
  "name": "@open-decision/type-classes",
  "version": "0.0.1",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "publishConfig": {
    "main": "./lib/index.cjs",
    "module": "./lib/index.mjs",
    "typings": "./lib/index.d.ts"
  },
  "files": [
    "/lib",
    "/src"
  ],
  "description": "The foundational types classes shared by all parts of the open decision application.",
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf lib & tsc --composite false --incremental false & vite build ",
    "prepublish": "pnpm build",
    "prettier": "prettier --check src/",
    "prettier:fix": "prettier --write src/",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "verify": "run-p prettier lint",
    "verify:fix": "yarn prettier:fix && yarn lint:fix"
  },
  "dependencies": {
    "zod": "^3.11.6",
    "json-stable-stringify": "^1.0.1",
    "murmurhash-js": "1.0.0"
  },
  "peerDependencies": {
    "ramda": "^0.27.1",
    "remeda": "^0.0.30"
  },
  "devDependencies": {
    "@types/ramda": "^0.27.46",
    "@types/json-stable-stringify": "^1.0.33",
    "@types/murmurhash-js": "^1.0.3",
    "remeda": "^0.0.30",
    "typescript": "^4.4.4",
    "utility-types": "^3.10.0",
    "vite": "^2.6.13"
  }
}
