{
  "name": "@open-decision/type-classes",
  "version": "0.0.1",
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "/lib",
    "/src"
  ],
  "description": "The foundational types classes shared by all parts of the open decision application.",
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf lib & tsc --composite false --incremental false & vite build",
    "dev": "tsc --composite false --incremental false & vite build --watch",
    "prepublish": "pnpm build",
    "prettier": "prettier --check src/",
    "prettier:fix": "prettier --write src/",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "verify": "run-p prettier lint",
    "verify:fix": "yarn prettier:fix && yarn lint:fix",
    "test": "jest"
  },
  "dependencies": {
    "immer": "^9.0.12",
    "json-stable-stringify": "^1.0.1",
    "murmurhash-js": "1.0.0",
    "ramda": "^0.28.0",
    "remeda": "^0.0.32",
    "utility-types": "^3.10.0",
    "uuid": "^8.3.2",
    "vite": "^2.8.6",
    "zod": "^3.13.4"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/json-stable-stringify": "^1.0.34",
    "@types/murmurhash-js": "^1.0.3",
    "@types/ramda": "^0.28.1",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "typescript": "^4.6.2"
  }
}
